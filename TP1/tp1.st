Object subclass: #AlgoVid	instanceVariableNames: 'listaPersonas listaBurbujas listaColegios'	classVariableNames: ''	package: 'tp1'!!AlgoVid methodsFor: 'puedeCircular'!puedeCircular: nombrePersona 	^(self buscar: nombrePersona enLista: listaPersonas ) puedeCircular.! !!AlgoVid methodsFor: 'nuevaPersona'!nuevaPersona: nombrePersona 	listaPersonas add: ((Persona new) nombre: nombrePersona ).! !!AlgoVid methodsFor: 'as yet unclassified'!esPersonalEsencial: nombrePersona	(self buscar: nombrePersona enLista: listaPersonas ) esPersonalEsencial.! !!AlgoVid methodsFor: 'as yet unclassified'!nuevaBurbuja: nombreBurbuja 	listaBurbujas add: ((Burbuja new) nombre: nombreBurbuja ).! !!AlgoVid methodsFor: 'as yet unclassified'!burbujaPinchada: nombreBurbuja 	^(self buscar: nombreBurbuja enLista: listaBurbujas ) estaPinchada.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'c 5/19/2021 12:27'!buscar: nombre enLista: lista 	^[lista at:nombre.] on:KeyNotFound do:[NombreInexistente signal].! !!AlgoVid methodsFor: 'as yet unclassified'!clasesPresencialesEn: nombreColegio 	^(self buscar: nombreColegio enLista: listaColegios ) clasesPresenciales.! !!AlgoVid methodsFor: 'as yet unclassified'!agregarA: nombrePersona  aLaBurbuja: nombreBurbuja 	(self buscar: nombreBurbuja  enLista: listaBurbujas  ) agregarA: 		(self buscar: nombrePersona  enLista: listaPersonas  ).	! !!AlgoVid methodsFor: 'as yet unclassified'!agregarA: nombreBurbuja  alColegio: nombreColegio	(self buscar: nombreColegio enLista: listaColegios ) agregarA: 		(self buscar: nombreBurbuja  enLista: listaBurbujas  ).! !!AlgoVid methodsFor: 'as yet unclassified'!contactoEstrechoEntre: unaPersona  y: otraPersona 	 (self buscar: unaPersona enLista: listaPersonas ) contactoEstrechoCon:		 (self buscar: otraPersona enLista: listaPersonas ).! !!AlgoVid methodsFor: 'as yet unclassified'!esPersonaDeRiesgo: nombrePersona 	(self buscar: nombrePersona enLista: listaPersonas ) esPersonaDeRiesgo.! !!AlgoVid methodsFor: 'as yet unclassified'!agregarSintoma: nombreSintoma  a: nombrePersona	(self buscar: nombrePersona enLista: listaPersonas ) nuevoSintoma: nombreSintoma .	! !!AlgoVid methodsFor: 'as yet unclassified'!initialize 	listaPersonas := Dictionary new.	listaBurbujas := Dictionary new.	listaColegios := Dictionary new.! !!AlgoVid methodsFor: 'as yet unclassified'!estaVacunadx: nombrePersona 	(self buscar: nombrePersona enLista: listaPersonas) vacunada .	! !!AlgoVid methodsFor: 'as yet unclassified'!nuevoColegio: nombreColegio	listaColegios  add: ((Colegio new) nombre: nombreColegio ).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 17:06'!diagnosticoDe: nombrePersona	^(self buscar:nombrePersona enLista: listaPersonas) diagnosticoResumido.! !Object subclass: #Burbuja	instanceVariableNames: 'nombre integrantes'	classVariableNames: ''	package: 'tp1'!!Burbuja methodsFor: 'initialization'!initialize 	integrantes := OrderedCollection new.! !!Burbuja methodsFor: 'accessing'!nombre: nombreBurbuja 	nombre := nombreBurbuja.! !!Burbuja methodsFor: 'accessing'!key 		^ nombre.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'Cristian 5/18/2021 14:38'!agregarA: persona 	integrantes add: persona.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 15:50'!confinarPor: personaEnferma	integrantes do:[:persona | ((persona diagnosticoEsPositivo) ifFalse:[persona contactoEstrechoCon: personaEnferma ])].! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 15:50'!estaPinchada	|personaEnferma |	personaEnferma := (integrantes detect: [ :persona | persona diagnosticoEsPositivo ] ifNone: [Persona new]).	(personaEnferma diagnosticoEsPositivo ) ifTrue: [self confinarPor: personaEnferma  ].	^(personaEnferma diagnosticoEsPositivo).	! !Object subclass: #Colegio	instanceVariableNames: 'nombre burbujas'	classVariableNames: ''	package: 'tp1'!!Colegio methodsFor: 'as yet unclassified'!agregarA: burbuja 	burbujas add: burbuja.	! !!Colegio methodsFor: 'as yet unclassified' stamp: 'Anonymous 5/18/2021 16:27'!clasesPresenciales	|totalBurbujasPinchadas|	totalBurbujasPinchadas := 0.	burbujas do:[:burbuja | 		(burbuja estaPinchada) ifTrue:[ totalBurbujasPinchadas:= totalBurbujasPinchadas  + 1]].	^((totalBurbujasPinchadas / (burbujas size)) * 100) < 40.! !!Colegio methodsFor: 'accessing'!nombre: nombreColegio 	nombre := nombreColegio .! !!Colegio methodsFor: 'accessing'!key 			^nombre.! !!Colegio methodsFor: 'initialization' stamp: 'Anonymous 5/18/2021 16:11'!initialize	burbujas := OrderedCollection new.! !Object subclass: #Diagnostico	instanceVariableNames: 'estado vacunada'	classVariableNames: ''	package: 'tp1'!!Diagnostico methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 19:28'!resumen	^(estado nombre).! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 19:27'!revisar: listaSintomas	estado := Sospechoso new.	((listaSintomas algunoHabitual and:(listaSintomas cantidad  >= 3)) or: 		(listaSintomas cantidad  >= 4)) ifTrue: [estado := Positivo new].! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:54'!initialize	estado := Sospechoso new.	vacunada := false.! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 16:32'!personaEsEsencial	estado := Sano new.! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 18:33'!estadoPositivo	^ (estado isMemberOf: Positivo).! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 18:58'!cambiarEstado	estado := Sospechoso new.! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:16'!personaVacunada	vacunada := true.	estado := Sano new.! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:17'!habilitaACircular	^estado habilitaACircular.! !Error subclass: #NombreInexistente	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!NombreInexistente class	instanceVariableNames: 'nombre'!Object subclass: #Persona	instanceVariableNames: 'nombre sintomas diagnostico personaDeRiesgo personaEsencial vacunada'	classVariableNames: ''	package: 'tp1'!!Persona methodsFor: 'as yet unclassified' stamp: 'Anonymous 5/18/2021 16:13'!nuevoSintoma: sintoma	sintomas agregar: sintoma.	diagnostico revisar: sintomas.! !!Persona methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 19:26'!contactoEstrechoCon: otraPersona.	(otraPersona puedeCircular) ifFalse:[		diagnostico cambiarEstado].! !!Persona methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 19:36'!vacunada	diagnostico personaVacunada.! !!Persona methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 17:20'!diagnosticoEsPositivo	^ diagnostico estadoPositivo.! !!Persona methodsFor: 'as yet unclassified' stamp: 'a 6/15/2021 19:28'!diagnosticoResumido	^ diagnostico resumen.! !!Persona methodsFor: 'puedeCircular' stamp: 'a 6/15/2021 16:35'!puedeCircular	^ personaDeRiesgo not and: diagnostico habilitaACircular.	! !!Persona methodsFor: 'puedeCircular' stamp: 'a 6/14/2021 19:00'!initialize 	sintomas := Sintomas new.	diagnostico := Diagnostico new.	vacunada := false.	personaDeRiesgo := false.	personaEsencial := false.! !!Persona methodsFor: 'nombre' stamp: 'Cristian 5/10/2021 23:24'!key 	^ nombre.! !!Persona methodsFor: 'accessing' stamp: 'c 5/19/2021 15:33'!esPersonaDeRiesgo	personaDeRiesgo := true.! !!Persona methodsFor: 'accessing' stamp: 'Cristian 5/10/2021 19:49'!nombre: nombreNuevo  	nombre := nombreNuevo.! !!Persona methodsFor: 'accessing' stamp: 'a 6/15/2021 16:32'!esPersonalEsencial	personaEsencial := true.	diagnostico personaEsEsencial.	! !Object subclass: #Positivo	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1'!!Positivo methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:46'!nombre 		^'Positivo'.! !!Positivo methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:47'!habilitaACircular 	^false.! !Object subclass: #Sano	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1'!!Sano methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:18'!habilitaACircular	^true.! !!Sano methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:45'!nombre 		^'Sano'.! !Object subclass: #Sintomas	instanceVariableNames: 'listaSintomas'	classVariableNames: ''	package: 'tp1'!!Sintomas methodsFor: 'initialization' stamp: 'Anonymous 5/18/2021 16:14'!initialize 		listaSintomas := OrderedCollection new.! !!Sintomas methodsFor: 'as yet unclassified' stamp: 'Anonymous 5/18/2021 16:13'!agregar: sintoma 	listaSintomas add: sintoma.! !!Sintomas methodsFor: 'as yet unclassified' stamp: 'Anonymous 5/18/2021 16:15'!algunoHabitual	^(listaSintomas anySatisfy:[ :nombreSintoma| nombreSintoma = 'Fiebre']) |	(listaSintomas anySatisfy:[ :nombreSintoma| nombreSintoma = 'Tos']) |	(listaSintomas anySatisfy:[ :nombreSintoma| nombreSintoma = 'Cansancio']).! !!Sintomas methodsFor: 'as yet unclassified' stamp: 'Anonymous 5/18/2021 16:17'!cantidad 	^(listaSintomas size).! !Object subclass: #Sospechoso	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1'!!Sospechoso methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:18'!habilitaACircular	^false.! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'a 6/14/2021 21:46'!nombre	^'Sospechoso'.! !